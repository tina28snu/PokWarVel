@model MarvelApi.Model.Characters


<table class="tableAvatar col-lg-4 col-xs-2 col-md-3">
    <tr>
        <td>
            <img src="@Model.Avatar">
        </td>
    </tr>
    <tr>
        <td>
            @Html.DisplayFor(modelItem => Model.name)
        </td>
    </tr>
    <tr>
        <td>
            @Html.DisplayFor(modelItem => Model.description)
        </td>
    </tr>
</table>




